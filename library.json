  "repository":
  {
    "type": "git",
    "url": "https://github.com/hoeken/PsychicHttp"
  },
  "authors":
  [
    {
      "name": "Zach Hoeken",
      "email": "hoeken@gmail.com",
      "maintainer": true
    }
  ],
  "examples": [
    {
